<input type="hidden" id="chart-data" value="{{initialInfo.chart.allTransactionForLastMonth}}">

<script>
    let day_data = JSON.parse($("#chart-data").val());
    Morris.Line({
        element: 'graph',
        data: day_data,
        xkey: '_id',
        ykeys: ['total'],
        labels: ['Total']
    });
</script>
