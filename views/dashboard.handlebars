                            <td>{{name}} {{lastname}}</td>
                            <td>{{idNumber}}</td>
                            <td>{{email}}</td>
                            <td>{{cellPhone}}</td>
                            <td><span class="badge badge-pill {{parseStatusBadgeColor status}}" style="color:#fff">{{parseStatus status}}</span></td>
